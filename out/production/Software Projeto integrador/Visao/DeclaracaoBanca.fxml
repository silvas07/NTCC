    <padding>
        <Insets bottom="20" left="10" right="10" top="20" />
    </padding>

    <Label alignment="CENTER" prefHeight="36.0" prefWidth="614.0" text="Declaração de banca de TCC " GridPane.columnSpan="5" GridPane.rowIndex="1">
        <font>
            <Font name="System Bold" size="25" />
        </font>
    </Label>

    <Label text="Nome professor orinetador:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <TextField fx:id="TextFieldProferssor" prefHeight="25.0" prefWidth="336.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <Label text="Matricula:" GridPane.columnIndex="2" GridPane.rowIndex="2" />
    <TextField fx:id="TextFieldMatriculaProfessor" prefHeight="25.0" prefWidth="191.0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
    <Button onAction="#filtrarProfessor" text="Filtrar" GridPane.columnIndex="4" GridPane.rowIndex="2" />


    <Label text="Titulo TCC:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <ComboBox fx:id="ComboBoxTituloTCC" prefHeight="25.0" prefWidth="553.0" GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="3" />

    <Label text="Nome do alumo:" GridPane.columnIndex="0" GridPane.rowIndex="4" />
    <TextField fx:id="TextFieldNomeAluno" prefHeight="25.0" prefWidth="315.0" GridPane.columnIndex="1" GridPane.columnSpan="1" GridPane.rowIndex="4" />
    <Label text="Matricula:" GridPane.columnIndex="2" GridPane.rowIndex="4" />
    <TextField fx:id="TextFieldMatriculaAluno" prefHeight="25.0" prefWidth="191.0" GridPane.columnIndex="3" GridPane.rowIndex="4" />
    <Button onAction="#filtrarAluno" text="Filtrar" GridPane.columnIndex="4" GridPane.rowIndex="4" />

    <HBox fx:id="HboxButoes" alignment="CENTER" spacing="5" GridPane.columnSpan="5" GridPane.rowIndex="5">
        <children>
            <Button onAction="#Buscar" text="Buscar" />
            <Button onAction="#cancelar" text="Cancelar" />
        </children>
    </HBox>


    <TableView prefHeight="385.0" prefWidth="800.0" GridPane.columnIndex="0" GridPane.columnSpan="6" GridPane.rowIndex="7">
        <columns>
            <TableColumn prefWidth="473.0" text="Titulo TCC" />
            <TableColumn prefWidth="95.0" text="Data Inicio" />
            <TableColumn prefWidth="93.0" text="TipoTCC" />
